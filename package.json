{
  "name": "@asafarim/direxpo-server",
  "version": "0.4.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/dev-server.ts",
    "demo": "pnpm run build && pnpm --filter direxpo-server-demo dev",
    "deploy:demo": "gh-pages -d demo/dist",
    "deploy": "pnpm run deploy:demo",
    "release": "node scripts/release.js",
    "start": "concurrently \"pnpm run dev\" \"pnpm run demo\""
  },
  "dependencies": {
    "@asafarim/md-exporter": "0.2.3",
    "@asafarim/direxpo-core": "^0.4.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "gh-pages": "^6.3.0",
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AliSafari-IT/direxpo-server.git"
  },
  "homepage": "https://alisafari-it.github.io/direxpo-server/",
  "keywords": [
    "direxpo",
    "file-discovery",
    "Tree Generator",
    "folder-tree",
    "markdown-export",
    "markdown",
    "utilities"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Ali Safari",
  "license": "MIT"
}